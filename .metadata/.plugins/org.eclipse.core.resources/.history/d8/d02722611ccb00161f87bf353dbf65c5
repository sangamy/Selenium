package library;

public class Example_Constructors {

	int age;
	String name;
	double income;
	
	
	// Default Constructor
	public Example_Constructors(){

		age = 1;
		name = "ABC";
		income= 30.265;
		
	}
	
	 
	// Parameterized Constructor

	public Example_Constructors(int a, String b){

		this.age = a;
		this.name = b;
	}
    
	public Example_Constructors(int a, String b, double c){
 		this.age = a;
		this.name = b;
		this.income = c;
	}
	
	
	public void printSomething(){

		System.out.println("a = "+this.age);
		System.out.println("b = "+this.name);
		

	}
	
	public void printsomething1(){
		
		System.out.println("a = "+this.age);
		System.out.println("b = "+this.name);
		System.out.println("c = "+this.income);
	}


	
	
	
}
